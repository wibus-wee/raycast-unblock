{
  "name": "raycast-unblock",
  "type": "module",
  "version": "0.1.0-beta.3",
  "packageManager": "pnpm@8.14.0",
  "description": "Unblock all features in Raycast Pro Plan",
  "author": "wibus-wee <wibus@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/wibus-wee/raycast-unblock#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wibus-wee/raycast-unblock.git"
  },
  "bugs": "https://github.com/wibus-wee/raycast-unblock/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "build:rollup": "rollup -c",
    "bundle": "sh scripts/single-application.sh",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp",
    "start": "esno src/index.ts",
    "start:dev": "esno watch src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@google/generative-ai": "^0.2.0",
    "consola": "^3.2.3",
    "destr": "^2.0.2",
    "dotenv": "^16.4.1",
    "fastify": "^4.26.0",
    "fastify-sse-v2": "^3.1.2",
    "ofetch": "^1.3.3",
    "openai": "^4.26.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.2",
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.7",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.10.7",
    "@types/uuid": "^9.0.8",
    "bumpp": "^9.2.1",
    "eslint": "^8.56.0",
    "esno": "^4.0.0",
    "lint-staged": "^15.2.0",
    "pnpm": "^8.14.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.6",
    "rollup-plugin-esbuild": "^6.1.1",
    "simple-git-hooks": "^2.9.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vite": "^5.0.11",
    "vitest": "^1.1.3"
  },
  "pnpm": {
    "patchedDependencies": {
      "fastify-sse-v2@3.1.2": "patches/fastify-sse-v2@3.1.2.patch"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
